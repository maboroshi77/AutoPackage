package org.account.autopackage;

import org.account.autopackage.vo.MultiRevisionVo;

public class AutoPackageConfige {
	private String eclipseProjectPath;
	private boolean isNeedDataChange;
	private String packeVersion;
	private String packagePath;
	private String packageName;
	private MultiRevisionVo updateSvnRec;
	
	public String getEclipseProjectPath() {
		return eclipseProjectPath;
	}
	public void setEclipseProjectPath(String eclipseProjectPath) {
		this.eclipseProjectPath = eclipseProjectPath;
	}
	public boolean isNeedDataChange() {
		return isNeedDataChange;
	}
	public void setNeedDataChange(boolean isNeedDataChange) {
		this.isNeedDataChange = isNeedDataChange;
	}
	public String getPackeVersion() {
		return packeVersion;
	}
	public void setPackeVersion(String packeVersion) {
		this.packeVersion = packeVersion;
	}
	public String getPackagePath() {
		return packagePath;
	}
	public void setPackagePath(String packagePath) {
		this.packagePath = packagePath;
	}
	public String getPackageName() {
		return packageName;
	}
	public void setPackageName(String packageName) {
		this.packageName = packageName;
	}
	public MultiRevisionVo getUpdateSvnRec() {
		return updateSvnRec;
	}
	public void setUpdateSvnRec(MultiRevisionVo updateSvnRec) {
		this.updateSvnRec = updateSvnRec;
	}
	
	public String getPackageDirPath(){
		return packagePath+packageName;
	}
}